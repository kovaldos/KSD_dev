mixin map(mods)
	-
		const data = {
			title: "Карта",
			subtitle: "География обслуживания",
		}
	section.section.map(class="map--" + mods)
		.container.map__container
			.map__wrapper 
				h2.title.title--h2.map__title=data.title
				if !mods
					h3.title.title--h3.map__subtitle=data.subtitle
				.map__map#contacts-map