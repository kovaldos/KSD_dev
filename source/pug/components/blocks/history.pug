	section.section.history 
		.container.history__container 
			.history__heading 
				h2.title.title--h2.history__title=data.title
				h3.title.title--h3.history__subtitle=data.subtitle
		.history__wrapper 
			.container.history__container.history__container--rel
				.tabs.history__tabs(data-tabs="parent" data-delay="300" data-height="max")
					.history__tabs-progressbar
						.history__tabs-progress
					nav.tabs__controls.history__tabs-controls(data-tabs="controls")
						each item, index in data.tabsNav
							button.btn.history__tabs-control(data-index=index, data-tabs="control", class!=item.className)
								span.history__tabs-control-value=item.text
					.tabs__content.history__tabs-content(data-tabs="content")
						each item in data.tabsContent
							.history__tabs-cols(data-tabs="element")
								.history__tabs-col.history__tabs-col--01
									h4.title.history__tabs-title=item.title
									p.text.text--sm.history__tabs-text=item.text
								.history__tabs-col.history__tabs-col--02
									.history__numbers 
										.history__numbers-item
											.history__numbers-num
												span=item.numb1
											.history__numbers-text
												span=item.numbText1
										.history__numbers-item
											.history__numbers-num
												span=item.numb2
											.history__numbers-text
												span=item.numbText2
										.history__numbers-item
											.history__numbers-num
												span=item.numb3
											.history__numbers-text
												span=item.numbText3


